@model Soc_Management_Web.Models.ReportFileInfo
<style>
    .devicer {
        width: 100%;
        display: block;
        height: 5px;
        background-color: #4b4b4d;
    }

    .devicer1 {
        width: 100%;
        display: block;
        height: 2px;
        background-color: #4b4b4d;
    }
</style>
<div class="col-md-12" style="margin-right:0px;margin-left:0px;width:100%">
    <div class="row">


        <div class="col-md-4" style='display: @(Model.Title !="Data not found" ? "block" : "none");'>
            <a href="~/Reports/@Model.FileName" id="bndownload1" target="_blank" class="btn btn-primary">Get Full Screen</a>
        </div>


        <div class="col-md-2">

        </div>
        <div class="col-md-2">

        </div>
    </div>
    <div class="row" style="margin-top:1%">
        <div class="col-md-1">

        </div>
        <div class="col-md-12">
            @if (Model.Title != "Data not found")
            {
            <iframe src="~/reports/@Model.FileName" allowfullscreen="true"
                    width="100%" height="900px"></iframe>
            }
            else
            {
        <h4> Data not available to show </h4>
           
            }
        </div>
    </div>


    <!-- Modal -->
    
</div>



   
