    function convertDecimalToDate(decimalDate) {
        var millisecondsPerDay = 24 * 60 * 60 * 1000; // Number of milliseconds in a day
        var epoch = new Date('1970-01-01'); // Unix epoch

        // Calculate the milliseconds offset from the epoch
        var offsetMilliseconds = decimalDate * millisecondsPerDay;

        // Calculate the date by adding the offset to the epoch
        var resultDate = new Date(epoch.getTime() + offsetMilliseconds);

        return resultDate.toISOString().slice(0, 10); // Get the date part in 'yyyy-mm-dd' format
    }

   
