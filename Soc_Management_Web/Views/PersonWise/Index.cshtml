        $('#txtoccasionfromDate').on('change', function () {
            var inputDate = $('#txtoccasionfromDate').val();

            // Check if the input is not empty
            if (inputDate !== '') {
                // Format the date
                var formattedDate = inputDate.replace(/(\d{2})(\d{2})(\d{4})/, '$1/$2/$3');

